generator client {
  provider = "prisma-client-js"
  output   = "./../generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model TestUser {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  password String   @unique
  name     String?
  roles    String[]
}
